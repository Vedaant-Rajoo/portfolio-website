{
  "name": "vedaant-rajoo.github.io",
  "version": "1.0.0",
  "description": "Personal portfolio website",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npx tailwindcss -i ./style.css -o ./output.css --watch\" \"webpack serve --mode development\"",
    "prebuild": "npx update-browserslist-db@latest",
    "build": "NEXT_PUBLIC_DISCORD_ID=$NEXT_PUBLIC_DISCORD_ID npx tailwindcss -i ./style.css -o ./output.css && webpack --mode production",
    "start": "webpack serve --mode production",
    "lint": "eslint .",
    "clean": "rm -rf dist/*"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": ["portfolio", "personal-website"],
  "author": "Vedaant Rajoo",
  "license": "ISC",
  "dependencies": {
    "caniuse-lite": "^1.0.30001699",
    "gsap": "^3.12.7",
    "tailwindcss": "^3.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "babel-loader": "^9.2.1",
    "concurrently": "^9.1.2",
    "dotenv-webpack": "^8.1.0",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  }
}
